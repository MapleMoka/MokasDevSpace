<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<GeneDef>
		<defName>Moka_AfroTextureHair</defName>
		<label>afro texture</label>
		<description>Carriers of this gene have any of the afrotextured hairs.</description>
		<iconPath>Moka/Core/AfroTexturedHair</iconPath>
		<displayOrderInCategory>60</displayOrderInCategory>
	    <selectionWeightFactorDarkSkin>1</selectionWeightFactorDarkSkin>
		<hairTagFilter>
			<whitelist>true</whitelist>
			<tags>
				<li>AfroTexture</li>
			</tags>
		</hairTagFilter>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>afro</symbol></li>
				<li><symbol>bantu</symbol></li>
				<li><symbol>dread</symbol></li>
				<li><symbol>coil</symbol></li>
				<li><symbol>braid</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>lock</symbol></li>
				<li><symbol>loc</symbol></li>
				<li><symbol>lox</symbol></li>
				<li><symbol>tie</symbol></li>
				<li><symbol>knot</symbol></li>
			</suffixSymbols>
		</symbolPack>
		<biostatCpx>0</biostatCpx>
		<displayCategory>Cosmetic</displayCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<exclusionTags>
			<li>HairStyle</li>
		</exclusionTags>
	</GeneDef>
	
	<GeneDef Name="Moka_ColoredHairs" ParentName="GeneHairColorBase" Abstract = "true">
		<selectionWeight>0</selectionWeight>
		<randomBrightnessFactor>0</randomBrightnessFactor>
		<iconPath>Moka/Core/ColoredAfroTex</iconPath>
	</GeneDef>

	<GeneDef>
		<defName>Moka_RandomGene_FloralHair</defName>
		<label>floral shuffle</label>
		<geneClass>MokaDevSpace.Gene_RandomGeneFromList</geneClass>
		<description>Carriers of this gene have latent grazer dna in them that can be triggered to transform their bodies.</description>
		<displayCategory>ToBeSorted</displayCategory>
		<iconPath>Moka/Nature/RandomFloralHairs</iconPath>
		<displayOrderInCategory>5.2</displayOrderInCategory>
		<biostatMet>1</biostatMet>
		<customEffectDescriptions>
			<li>On generation, this gene is replaced with a random gene that changes a pawns limbs and eyes in line with specific animals.</li>
		</customEffectDescriptions>
		<modExtensions>
			<li Class="MokaDevSpace.Ext_RandomGeneFromList">
				<genesToChooseFrom>
					<li>Moka_HairColor_EarlyAutumn</li>
					<li>Moka_HairColor_DeepAutumn</li>
					<li>Moka_HairColor_NewLeaf</li>
					<li>Moka_HairColor_PaleLeaf</li>
					<li>Moka_HairColor_LightSakura</li>
					<li>Moka_HairColor_DarkSakura</li>

					<li>Moka_HairColor_LightGreen</li>
					<li>Moka_HairColor_LightGreen</li>
					<li>Moka_HairColor_LightGreen</li>
					
					<li>Moka_HairColor_RichGreen</li>
					<li>Moka_HairColor_RichGreen</li>
					<li>Moka_HairColor_RichGreen</li>
					
					<li>Moka_HairColor_StrikingGreen</li>
					<li>Moka_HairColor_StrikingGreen</li>
					<li>Moka_HairColor_StrikingGreen</li>
					
					<li>Moka_HairColor_PaleGreen</li>
					<li>Moka_HairColor_PaleGreen</li>
					<li>Moka_HairColor_PaleGreen</li>
					
					<li>Moka_HairColor_DeepGreen</li>
					<li>Moka_HairColor_DeepGreen</li>
					<li>Moka_HairColor_DeepGreen</li>
				</genesToChooseFrom>
			</li>
		</modExtensions>
	</GeneDef>
	
