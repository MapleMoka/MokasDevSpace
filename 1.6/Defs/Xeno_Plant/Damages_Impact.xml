<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<DamageDef>
		<defName>Shred</defName>
		<label>shred</label>
		<workerClass>DamageWorker_Cut</workerClass>
		<externalViolence>true</externalViolence>
		<deathMessage>{0} has been shredded to death.</deathMessage>
		<hediff>Shredded</hediff>
		<hediffSolid>Cut</hediffSolid>
		<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
		<impactSoundType>Slice</impactSoundType>
		<armorCategory>Sharp</armorCategory>
		<overkillPctToDestroyPart>0~0.8</overkillPctToDestroyPart>
		<cutExtraTargetsCurve>
			<points>
				<li>0, 0</li>
				<li>0.6, 1</li>
				<li>0.9, 2</li>
				<li>1, 3</li>
			</points>
		</cutExtraTargetsCurve>
		<cutCleaveBonus>1.8</cutCleaveBonus>
	</DamageDef>

	<DamageDef>
		<defName>Moka_Lash</defName>
		<label>lash</label>
		<workerClass>DamageWorker_AddInjury</workerClass>
		<externalViolence>true</externalViolence>
		<deathMessage>{0} has been crushed to death.</deathMessage>
		<hediff>Moka_Lash</hediff>
		<!-- <hediffSkin>Cut</hediffSkin> -->
		<hediffSolid>Crack</hediffSolid>
		<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
		<impactSoundType>Blunt</impactSoundType>
		<armorCategory>Blunt</armorCategory>
		<overkillPctToDestroyPart>0~0.2</overkillPctToDestroyPart>
	</DamageDef>

	<DamageDef Name="LeafCutBase">
		<defName>Moka_LeafCut</defName>
		<label>leaf cut</label>
		<workerClass>DamageWorker_Cut</workerClass>
		<externalViolence>true</externalViolence>
		<deathMessage>{0} has been cut to death.</deathMessage>
		<hediff>Cut</hediff>
		<hediffSkin>Cut</hediffSkin>
		<hediffSolid>Crack</hediffSolid>
		<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
		<impactSoundType>Slice</impactSoundType>
		<armorCategory>Sharp</armorCategory>
		<overkillPctToDestroyPart>0~0.05</overkillPctToDestroyPart>
		<cutExtraTargetsCurve>
			<points>
				<li>0, 0</li>
				<li>0.6, 2</li>
				<li>0.9, 3</li>
				<li>1, 5</li>
			</points>
		</cutExtraTargetsCurve>
		<cutCleaveBonus>1.2</cutCleaveBonus>
	</DamageDef>

	<DamageDef ParentName="LeafCutBase">
		<defName>Moka_ToxicLeafCut</defName>
		<label>toxic leaf cut</label>
		<additionalHediffs>
			<li>
				<hediff>ToxicBuildup</hediff>
				<severityPerDamageDealt>0.005</severityPerDamageDealt>
				<victimSeverityScaling>ToxicResistance</victimSeverityScaling>
				<inverseStatScaling>true</inverseStatScaling>
				<victimSeverityScalingByInvBodySize>true</victimSeverityScalingByInvBodySize>
			</li>
		</additionalHediffs>
		<applyAdditionalHediffsIfHuntingForFood>false</applyAdditionalHediffsIfHuntingForFood>
		<impactSoundType>Toxic</impactSoundType>
		<damageEffecter>Impact_Toxic</damageEffecter>
	</DamageDef>

	<DamageDef ParentName="BluntBase">
		<defName>Moka_CactusSmash</defName>
		<label>cactus slam</label>
		<additionalHediffsThisPart> <!-- Moka_CactusSpines -->
			<li>PorcupineQuill</li>
		</additionalHediffsThisPart>
		<applyAdditionalHediffsIfHuntingForFood>true</applyAdditionalHediffsIfHuntingForFood>
	</DamageDef>

