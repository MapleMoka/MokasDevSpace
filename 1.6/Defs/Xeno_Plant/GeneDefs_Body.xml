	<!--==========					Antlers					==========-->
	<GeneDef ParentName="GeneHeadboneBase">
		<defName>Moka_Headbone_BarkAntlers</defName>
		<label>bark-antlers</label>
		<description>Carriers of this gene grow two small horns mimicking those of cervids from old earth.</description>
		<iconPath>Moka/Nature/Antlers/BarkAntlers</iconPath>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<texPaths>
				<!-- Hacky way to make certain horn-types less common -->
					<li>Moka/Nature/Antlers/BarkAntlers_1</li>
					<li>Moka/Nature/Antlers/BarkAntlers_1</li>
					<li>Moka/Nature/Antlers/BarkAntlers_1</li>
