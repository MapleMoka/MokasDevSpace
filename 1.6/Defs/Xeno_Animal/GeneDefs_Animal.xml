	<GeneDef>
		<defName>Moka_RuminantDigestion</defName>
		<label>ruminant digestion</label>
		<description>Carriers of this gene are born with large and hardy digestive systems, allowing them to eat hay. They will eat hay when there are no fine meals around, however, they require a lot more food to feel full.</description>
		<customEffectDescriptions>
			<li>Can eat hay.</li>
		</customEffectDescriptions>
		<biostatCpx>2</biostatCpx>
		<biostatMet>1</biostatMet>
		<displayCategory>ToBeSorted</displayCategory>
		<displayOrderInCategory>11</displayOrderInCategory>
		<iconPath>Moka/Animal/BovineStomach</iconPath>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<dontMindRawFood>true</dontMindRawFood>
		<statFactors>
			<RawNutritionFactor>1.2</RawNutritionFactor>
			<MaxNutrition>1.2</MaxNutrition>
		</statFactors>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>graze</symbol></li>
				<li><symbol>hay</symbol></li>
				<li><symbol>herbi</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>grazer</symbol></li>
				<li><symbol>vore</symbol></li>
				<li><symbol>dae</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef>
		<defName>Moka_CaravanReadiness</defName>
		<label>caravan readiness</label>
		<iconPath>Moka/Animal/BovineStrength</iconPath>
		<description>Carriers of this gene form stronger muscles on their backs and shoulders, and have tougher spines, allowing them to haul more weight over long distances.</description>
		<displayCategory>ToBeSorted</displayCategory>
		<displayOrderInCategory>50</displayOrderInCategory>
		<statOffsets>
			<CarryingCapacity>25</CarryingCapacity>
			<VEF_MassCarryCapacity MayRequire="oskarpotocki.vanillafactionsexpanded.core">25</VEF_MassCarryCapacity>
		</statOffsets>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>heavy</symbol>
				</li>
				<li>
					<symbol>strong</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>weight</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

