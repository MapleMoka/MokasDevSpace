<Defs>

	<GeneDef>
		<defName>Moka_ExcessLactation</defName>
		<label>excess lactation</label>
		<description>Carriers of this gene produce milk at all times and can be milked by other pawns every now and then.</description>
		<geneClass>MokaDevSpace.Gene_ProduceFromGene</geneClass>
		<iconPath>Moka/Animal/ExcessLactation</iconPath>
		<displayCategory>ToBeSorted</displayCategory>
		<displayOrderInCategory>70</displayOrderInCategory>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<minAgeActive>16</minAgeActive>
		<modExtensions>
			<li Class="MokaDevSpace.Ext_ProduceFromGene">
				<produces>Milk</produces>
				<intervalDays>10</intervalDays>
				<amount>15</amount>
			</li>
		</modExtensions>
		<randomChosen>true</randomChosen>
		<exclusionTags>
			<!-- <li>Milkables</li> -->
		</exclusionTags>
	</GeneDef>
	
