	<GeneDef ParentName="Moka_BodyBase">
		<defName>Moka_BovineNose</defName>
		<label>bovine nose</label>
		<labelShortAdj>bovbody</labelShortAdj>
		<description>Carriers of this gene have their skin resembling tree bark. This allows them to withstand blunt trauma that would normally knock people unconscious.</description>
		<biostatCpx>1</biostatCpx>
		<!-- <biostatMet>-1</biostatMet> -->
		<selectionWeight>0</selectionWeight>
		<iconPath>Moka/Animal/BovineNose</iconPath>
		<forcedHeadTypes>
			<li>Moka_Bovine_AverageMasc_Norm</li>
			<li>Moka_Bovine_AverageMasc_Wide</li>
			
