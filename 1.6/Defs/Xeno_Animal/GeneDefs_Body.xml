	<GeneDef ParentName="GeneEarsBase">
		<defName>Moka_FloppyEars</defName>
		<label>floppy ears</label>
		<description>Carriers of this gene have drooping, floppy ears like a goat's.</description>
		<iconPath>Moka/Animal/BovineEars/BovineEars</iconPath>
		<renderNodeProperties>
			<li>
				<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
				<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
				<!-- <texPath>Moka/Animal/BovineEars//BovineEars_1</texPath> -->
				<texPaths>
					<li>Moka/Animal/BovineEars/BovineEars_1</li>
					<li>Moka/Animal/BovineEars/BovineEars_2</li>
