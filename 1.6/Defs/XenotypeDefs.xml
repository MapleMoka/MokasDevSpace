    <combatPowerFactor>1.5</combatPowerFactor>
    <nameMaker>NamerPersonPigskin</nameMaker>
    <nameMaker>NamerPersonDirtmole_Male</nameMaker>
    <nameMakerFemale>NamerPersonDirtmole_Female</nameMakerFemale>
