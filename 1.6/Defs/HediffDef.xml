<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
	<HediffDef>
		<defName>Moka_FloralRegeneration</defName>
		<label>floral regeneration</label>
		<description>This pawn has large fat stores capable of accumulating usable fat for dealing with starvation.</description>
		<hediffClass>HediffWithComps</hediffClass>
		<initialSeverity>1</initialSeverity>
		<everCurableByItem>false</everCurableByItem>
		<comps>
			<li Class="MokaDevSpace.HediffCompProperties_FloralRegeneration">
				<!-- <startThreshold>0.8</startThreshold>
				<maxStarch>0.8</maxStarch>
				<starvationThreshold>0.1</starvationThreshold>
				<starvationSpeed>0.01</starvationSpeed>
				<conversionSpeed>0.0005</conversionSpeed> -->

				<daysToHeal>5</daysToHeal>
				<daysToGenerate>15</daysToGenerate>
				<amountToHeal>5</amountToHeal>
			</li>
		</comps>
	</HediffDef>
  
	<HediffDef>
		<defName>Moka_StarchDeposits</defName>
		<label>starch deposits</label>
		<description>This pawn has large fat stores capable of accumulating usable fat for dealing with starvation.</description>
		<hediffClass>MokaDevSpace.Hediff_Starch</hediffClass>
		<initialSeverity>1</initialSeverity>
		<everCurableByItem>false</everCurableByItem>
		<comps>
			<li Class="MokaDevSpace.HediffCompProperties_Starch">
				<startThreshold>0.8</startThreshold>
				<maxStarch>0.8</maxStarch>
				<starvationThreshold>0.1</starvationThreshold>
				<starvationSpeed>0.01</starvationSpeed>
				<conversionSpeed>0.0005</conversionSpeed>
				<!-- adjustment curves -->
				<speedUpCurve>
					<points>
						<li>(0, 0)</li>
						<li>(0.10, -0.01)</li>
						<li>(0.20, -0.05)</li>
						<li>(0.30, -0.07)</li>
						<li>(0.40, -0.10)</li>
					</points>
				</speedUpCurve>
				<tirednessReductionCurve>
					<points>
						<li>(0, 0)</li>
						<li>(0.10, 0.04)</li>
						<li>(0.20, 0.08)</li>
						<li>(0.30, 0.12)</li>
						<li>(0.40, 0.15)</li>
					</points>
				</tirednessReductionCurve>
				<learningRateCurve>
					<points>
						<li>(0, 0)</li>
						<li>(0.10, 5)</li>
						<li>(0.20, 10)</li>
						<li>(0.30, 20)</li>
						<li>(0.40, 30)</li>
					</points>
				</learningRateCurve>
			</li>
		</comps>
	</HediffDef>
  
